./gauss_corr sample num_samples=100000 num_warmup=1000 algorithm=hmc stepsize=0.1 \
metric=unit_e engine=exhaustive max_depth=20 x_delta=0.01 adapt engaged=0 init=0 \
random seed=2282713988 output file=gauss_corr.exhaustive.2.csv refresh=10000

../../bin/print --sig_figs=5 gauss_corr.exhaustive.2.csv | \
awk 'NR > 14 && NR < 115 {print $1, $8, $9}' > gauss_corr.exhaustive.2.ess.dat

#paste gauss_corr.nuts.ess.dat /dev/null gauss_corr.exhaustive.1.ess.dat /dev/null gauss_corr.exhaustive.2.ess.dat > gauss_corr.ess.dat
