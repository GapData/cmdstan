./irt sample num_samples=100000 num_warmup=1000 algorithm=hmc stepsize=0.1 \
metric=unit_e engine=nuts max_depth=20 adapt engaged=0 init=0 data file=irt-data.R \
random seed=2282713988 output file=irt.nuts.csv refresh=10000

../../bin/print --sig_figs=5 irt.nuts.csv | \
awk 'NR > 14 && NR < 135 {print $1, $8, $9}' > irt.nuts.ess.dat

#paste irt.nuts.ess.dat /dev/null irt.exhaustive.1.ess.dat /dev/null irt.exhaustive.2.ess.dat > irt.ess.dat

