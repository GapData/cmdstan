./gauss_corr sample num_samples=100000 num_warmup=1000 algorithm=hmc stepsize=0.1 \
metric=unit_e engine=static_uniform num_leapfrog=$1 adapt engaged=0 init=0 \
random seed=2282713988 output file=gauss_corr.static_uniform.$1.csv refresh=10000

../../bin/print --sig_figs=5 gauss_corr.static_uniform.$1.csv | \
awk 'NR > 12 && NR < 113 {print '$1', $8, $9}' > gauss_corr.static_uniform.$1.ess.dat

