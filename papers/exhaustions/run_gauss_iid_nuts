./gauss_iid sample num_samples=10000 num_warmup=1000 algorithm=hmc stepsize=0.1 \
metric=unit_e engine=nuts max_depth=20 adapt engaged=0 init=0 \
random seed=2282713988 output file=gauss_iid.nuts.csv refresh=1000

../../bin/print --sig_figs=5 gauss_iid.nuts.csv | \
awk 'NR > 13 && NR < 134 {print $1, $8}' > gauss_iid.nuts.ess.dat

#paste irt.nuts.ess.dat /dev/null irt.exhaustive.1.ess.dat /dev/null irt.exhaustive.2.ess.dat > irt.ess.dat

